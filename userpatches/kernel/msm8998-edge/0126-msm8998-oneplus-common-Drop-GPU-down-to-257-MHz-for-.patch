+			wakeup-source; // TODO: remove?
 		};
 	};
 
@@ -662,6 +662,8 @@ opp-table {
 		/delete-node/ opp-670000048;
 		/delete-node/ opp-596000097;
 		/delete-node/ opp-515000097;
